{
  "version": 6,
  "include": [
    "base.json"
  ],
  "configurePresets": [
    {
      "name": "macos-clang-x64-dbg",
      "displayName": "MacOS Clang x64 Debug",
      "inherits": "macos-clang-base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_OSX_ARCHITECTURES":"x86_64"
      }
    },
    {
      "name": "macos-clang-x64-rel",
      "displayName": "MacOS Clang x64 Release",
      "inherits": "macos-clang-base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_OSX_ARCHITECTURES":"x86_64"
      }
    }
  ]
}
